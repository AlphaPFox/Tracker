<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="16dp"
        android:left="16dp"
        android:right="16dp"
        android:top="16dp">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners
                android:radius="3dp" />
            <solid android:color="#FFF" />
        </shape>
    </item>
    <item
        android:top="35dp"
        android:bottom="2dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:dashWidth="3dp"
                    android:dashGap="3dp"
                    android:width="2dp"
                    android:color="#FFF" />
            </shape>
        </rotate>
    </item>
</layer-list>